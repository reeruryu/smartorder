<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.smartorder.admin.mapper.StoreMapper">

  <select id="selectListCount" resultType="long">
    select count(*)
    from store
  </select>

  <select id="selectList" resultType="com.example.smartorder.admin.dto.StoreDto">
    select *
    from store
    limit #{pageStart}, #{pageEnd}
  </select>

</mapper>